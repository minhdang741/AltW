{
  "name": "AltW for Chrome",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Auto Close for Chrome",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "scripts/main.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage"
  ]
}